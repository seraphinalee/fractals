p = 0.5;
q = 1-p;
m = 5;
cutoff = 0;

[xcors,laplacian] = intervallapgen(m,p,1-p,cutoff); %m,p,q,cutoff
[x,eigvals,V] = fullspectra(laplacian);
V = [zeros(1,length(V)); V; zeros(1,length(V))];




for i=1:10
    
    subplot(4,2,1)
    zeroplotter(xcors,V(:,1))
    subplot(4,2,2)
    zeroplotter(xcors,V(:,2))
    subplot(4,2,3)
    zeroplotter(xcors,V(:,3))
    subplot(4,2,4)
    zeroplotter(xcors,V(:,4))
    subplot(4,2,5)
    zeroplotter(xcors,V(:,5))
    subplot(4,2,6)
    zeroplotter(xcors,V(:,6))
    subplot(4,2,7)
    zeroplotter(xcors,V(:,7))
    subplot(4,2,8)
    zeroplotter(xcors,V(:,8))
end


















function res = edgeresistance( x, y, ra )
%x and y are points
neighbors = pointneighbors(x);
is_neighbor = FALSE;

for i=1:len(neighbors)
    if equals(y, neighbors(i))
        is_neighbor = TRUE;
    end
end

if not is_neighbor
    res = 0
    return
end

